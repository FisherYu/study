.icon-goback,
.icon-go{
	background: url(//pic.c-ctrip.com/platform/h5/my/order/un_ico_order@2x_v6.20.png) no-repeat 60px 60px;
    display: inline-block;
    vertical-align: middle;
    margin: 0 8px;
    background-size: 60px auto;
}
.icon-goback {
    width: 15px;
    height: 11px;
    background-position: 0 0;
}
.icon-go {
    width: 15px;
    height:5px;
    background-position: 0 -40px;
}
.order-cancel .icon-goback{
	background-position:  -20px 0;
}
.order-cancel .icon-go{
	background-position: -20px -40px;
}
.user-info{
	width: 100%;
	height: 78rpx;
	padding: 15rpx 30rpx;
	box-sizing:border-box;
	color: #fff;
	background: rgba(0,0,0,.25);
	overflow: hidden;
	vertical-align: middle;
}
.user-info_whitebg{
	background: rgba(255,255,255,.25);
}
.user-desc{
	float: left;
	display: inline-block;
}
.user-name{
	display: inline-block;
	max-width: 320rpx;
	text-align: left;
	height: 40rpx;
	line-height: 40rpx;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	vertical-align: middle;
	font-size: 28rpx;
}
.user-exit{
	position: relative;
	display: inline-block;
	font-size: 24rpx;
	border-radius:20rpx;
	padding:0 15rpx;
	margin-left:15rpx;
	height: 40rpx;
	line-height: 40rpx;
	border: 1px solid #fff;
	vertical-align: middle;

}
.user-exit:after{
	content: '';
	position: absolute;
	top: -13rpx;
	bottom: -13rpx;
	left: -30rpx;
	right: -30rpx;
}
.user-service{
	float: right;
	height: 44rpx;
	line-height: 44rpx;
	display: inline-block;
	font-size: 30rpx;
	border: 1px solid #fff;
	border-radius:20rpx;
	padding:0 16rpx;
	vertical-align: middle;
}
.bu-order{
    margin:16rpx 16rpx 10rpx;
    padding:30rpx;
    background: #fff;
    border-radius: 12rpx;
}
.order-cont{
    position: relative;
	padding-right: 200rpx;
	margin-bottom: 30rpx;
	margin-top:-30rpx;
	overflow: hidden;
}
.order-rcon{
	position: absolute;
	width: 200rpx;
	right:0;
	top:30rpx;
	text-align: right;
}
.order-title{
	display: block;
	white-space: nowrap;
	text-overflow:ellipsis;
	overflow: hidden;
	font-size: 32rpx;
	color: #333;
	font-weight: bold;
	margin-top:30rpx;
}
.order-title text{
	vertical-align: middle;
}
.order-desc{
	display: block;
	margin-top:15rpx;
	font-size: 28rpx;
	color: #333;
	white-space: nowrap;
	text-overflow:ellipsis;
	overflow: hidden;
}
.order-desc-s{
	display: block;
	margin-top:10rpx;
	color: #999;
	font-size: 28rpx;
	white-space: nowrap;
	text-overflow:ellipsis;
	overflow: hidden;
}
.order-starttime{
	padding-right: 10rpx;
}
.order-endtime{
	padding:0 10rpx;
}
.order-price{
	color:#FF7D13;
	font-size: 32rpx;
	font-weight: bold;
}
.price-yen{
	font-size: 20rpx;
	vertical-align: bottom;
}
.order-status{
	color: #666;
	font-size: 26rpx;
}
.order-info{
	display: block;
	margin-top:16rpx;
	color: #999;
	font-size: 28rpx;
	white-space: nowrap;
	text-overflow:ellipsis;
	overflow: hidden;
}
.order-oper{
	margin-top:30rpx;
}
.order-oper:before,
.order-oper:after{
	content:'';
	display: table;
}
.order-oper:after{clear: both;}
.btn_mini{
	float: right;
	margin-left:20rpx;
}
.order-detail{
	position: relative;
	margin-top:15rpx;
}

.order-go,
.order-back{
	padding-left: 44rpx;
}
.order-go:before,
.order-back:before{
	position: absolute;
	left: 0;
	top: 0;
	width: 52rpx;
	height: 52rpx;
	line-height: 52rpx;
	border: 1px solid rgba(102,102,102,.7);
	border-radius: 4rpx;
	content:'去';
	color: #666;
	font-size: 40rpx;
	text-align: center;
	transform: scale(0.5) translate(-50%,-50%);
}
.order-back:before{
	content:'返';
}
.more-order{
	display: block;
	margin-top: 30rpx;
	text-align: center;
	font-size: 26rpx;
	color:rgba(255,255,255,.7);
}


/*没有订单*/
.failure-order_text{
	display: block;
	color: #999;
	font-size: 26rpx;
	text-align: center;
	margin-top: 20rpx;
}

/*已取消订单样式*/
.order-cancel,
.order-cancel:before{
	color: #bbb;
	border-color: #bbb;
}

.cui-mask {
    z-index: 2000;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    height: 100%;
    width: 100%;
    background-color: rgba(0,0,0,.75)
}
.g-m--c {
    position: absolute;
    width: 2.4rem;
    height: 2.4rem;
    left: 50%;
    top: 50%;
    margin: -1.25rem 0 0 -1.25rem;
    -webkit-animation: cui-loading 1.58s linear infinite;
    animation: cui-loading 1.58s linear infinite;
    border-top: 2px solid #fff;
    border-left: 2px solid #fff;
    border-radius: 50%
}

.g-m--l {
    position: absolute;
    width: 1.4rem;
    height: 1.4rem;
    left: 50%;
    top: 50%;
    z-index: 9;
    margin: -.75rem 0 0 -.6rem;
    background: url(data:image/png;base64,) no-repeat;
    background-size: 1.2rem 1.35rem
}
@-webkit-keyframes cui-loading {
    0% {
        opacity: 1;
        -webkit-transform: rotate(0deg)
    }

    100% {
        opacity: 1;
        -webkit-transform: rotate(360deg)
    }
}

@keyframes cui-loading {
    0% {
        opacity: 1;
        transform: rotate(0deg)
    }

    100% {
        opacity: 1;
        transform: rotate(360deg)
    }
}
